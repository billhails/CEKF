let
    link "listutils.fn" as list;

    fn enq (t, #(head, tail)) {
        #(head, t @ tail)
    }

    fn deq {
        (#(h @ head, tail)) {
            #(just(h), #(head, tail))
        }
        (x = #(_, [])) {
            #(nothing, x)
        }
        (#(_, tail)) {
            deq(#(list.reverse(tail), []))
        }
    }
in
    print(deq(enq(1, enq(2, enq(3, #([], []))))))
