namespace

link "../listutils.fn" as lst;
infix left 80 "|>" fn (l, f) { lst.map(f, l) };

typedef S {
    Atom(char) | Cons(char, list(S))
}

print S(s) {
let
    fn h {
        (Atom(c)) { putc(c) }
        (Cons(c, args)) {
            putc('(');
            putc(c);
            args |> fn (arg) {
                putc(' ');
                h(arg);
            };
            putc(')');
        }
    }
in
    h(s)
}
