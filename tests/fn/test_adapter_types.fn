// Test to see the types of lazy and adapter functions

let
    lazy fn lazy_or(a, b) {
        a or b
    }

    fn strict_lazy_or(x, y) {
        lazy_or(x, y)
    }
in
    assert((typeof lazy_or) == "#() -> bool -> #() -> bool -> bool");
    assert((typeof strict_lazy_or) == "bool -> bool -> bool");