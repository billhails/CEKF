// Comprehensive test of the typeof operator

let x = 42;
    y = 'a';
    f = fn (x) { x + 1 };
    lst = [1, 2, 3];
    tup = #(1, "hello");
    id = fn (x) { x };
in
    // Test basic types
    assert(typeof x == "number");
    assert(typeof y == "char");
    
    // Test function types
    assert(typeof f == "number -> number");
    
    // Test list types
    assert(typeof lst == "list(number)");