let
    fn multiValue() {
        #(1, 2, 3)
    }
    fn noValue() {
        #()
    }
in {
    let
        #(a, b, c) = multiValue();
        #() = noValue();
    in
        assert(a == 1 and b == 2 and c == 3);
        assert(typeof a == "number");
        assert(typeof b == "number");
        assert(typeof c == "number");
}