// Negative test: importing a fixity that already exists locally must fail
let
    // Local prefix operator ~
    fn negate(x) { 0 - x };
    prefix 13 "~" negate;

    // Library also exports prefix "~"
    link "import_ops_lib.fn" as ops;
    import ops prefix "~";  // should fail: redefinition conflict
in
    puts("This line should not be reached due to import conflict\n");
